	<queryString>
		<![CDATA[SELECT  F.MEDIDOR MEDIDOR, F.NOMBRE_CLIENTE CLIENTE, F.DIRECCION DIRECCION,
  NVL(F.CUENTA_ALCANTARILLADO,'--') CALCANTARILLADO,  --INFO GENERAL
    NVL( DF.RAZON_SOCIAL,'--') RAZONSOC, EM.ESTADO ESTMEDIDOR,  NVL(DF.LECTURA_MEDIDOR, 0) LECMEDIDOR,  NVL(DF.MED_IZQUIERDO,'--') MEDIZQ,  NVL(DF.MED_DERECHO,'--') MEDDERECHO, --CARGADATOS
    NVL( DF.NOM_ENTREVISTADO,'--') NOMBENTREVISTA,  NVL(DF.TEL_CONTACTO,'--') TELENTREVISTA,
     CASE WHEN  F.SIGV2TIPOSTELEFONOID IS NOT NULL THEN
        (SELECT TIPO  FROM VICMAN.SIGV2_TIPOS_TELEFONO WHERE SIGV2TIPOSTELEFONOID = F.SIGV2TIPOSTELEFONOID)
        ELSE '--'
      END TIPOTELENTREVISTA, nvl(DF.EMAIL,'--') MAILENTREVISTA,-- INFO ENTREVISTA
    DECODE(DF.USO_SUELO,
            'H', 'HABITADO',
            'D', 'DESHABITADO',
            'E', 'EN CONSTRUCCION',
            'B', 'BALDIO',
            '--') USOSUELO, NVL(DF.HABITANTES, 0) CANTHABITANTES, NVL(DF.TRABAJADORES, 0) CANTTRABAJADORES, --INFO USO
    --INICIA INFO TIPO DEL USO
    CASE WHEN  DF.SUBT_PRIVADOID IS NOT NULL THEN
        (SELECT SUB_TIPO FROM VICMAN.SIGV2_SUB_TIPOS_USOS WHERE SIGV2SUBTIPOSUSOSID = DF.SUBT_PRIVADOID)
        ELSE '--'
      END PRIVADO,
       DECODE(DF.GOBIERNO, 1, 'SI', 'NO') GOBIERNO, DECODE(DF.FRACCIONAMIENTO, 1, 'SI', 'NO') FRACCIONAMIENTO,
    CASE
         WHEN  DF.UNIFAMILIAR = '1' THEN 'UNIFAMILIAR'
         WHEN  DF.CONDOMINIO_VERTICAL = '1' THEN 'CONDOMINIO VERTICAL'
         WHEN  DF.CONDOMINIO_HORIZONTAL = '1' THEN 'CONDOMINIO HORIZONTAL'
     ELSE '--' END  VIVIENDA,     NVL(DF.NIVELES, 0) NIVELVIVIENDA,
    CASE WHEN  DF.SUBT_COMERCIOID IS NOT NULL THEN
        (SELECT SUB_TIPO FROM VICMAN.SIGV2_SUB_TIPOS_USOS WHERE SIGV2SUBTIPOSUSOSID = DF.SUBT_COMERCIOID)
        ELSE '--'
      END VENTAPRODUCTO,     NVL(DF.NUM_LOCALES, 0) CATLOCALES,
    CASE WHEN  DF.SUBT_SERVICIOID IS NOT NULL THEN
        (SELECT SUB_TIPO FROM VICMAN.SIGV2_SUB_TIPOS_USOS WHERE SIGV2SUBTIPOSUSOSID = DF.SUBT_SERVICIOID)
        ELSE '--'
      END SERVICIOS,
    CASE WHEN  DF.SUBT_ALIMENTOSID IS NOT NULL THEN
        (SELECT SUB_TIPO FROM VICMAN.SIGV2_SUB_TIPOS_USOS WHERE SIGV2SUBTIPOSUSOSID = DF.SUBT_ALIMENTOSID)
        ELSE '--'
      END ALMBEBIDAS,
    CASE WHEN  DF.SUBT_INDUSTRIAID IS NOT NULL THEN
        (SELECT SUB_TIPO FROM VICMAN.SIGV2_SUB_TIPOS_USOS WHERE SIGV2SUBTIPOSUSOSID = DF.SUBT_INDUSTRIAID)
        ELSE '--'
      END INDUSTRIA,
    CASE WHEN  DF.SUBT_OTROSID IS NOT NULL THEN
        (SELECT SUB_TIPO FROM VICMAN.SIGV2_SUB_TIPOS_USOS WHERE SIGV2SUBTIPOSUSOSID = DF.SUBT_OTROSID)
        ELSE '--'
      END OTROS, nvl(DF.OBSERVACIONES, '--') OBSER1, --FIN INFO TIPO DEL USO
    -- INICIA INFO ABASTECIMIENTO
    DECODE(DF.EMPAGUA, 1, 'SI', 'NO') EMPAGUA, DECODE(DF.MARISCAL, 1, 'SI', 'NO') MARISCAL, DECODE(DF.POZO_PROPIO, 1, 'SI', 'NO') POZOPROPIO,
     CASE WHEN  DF.PIPAS = 1 THEN
        DF.NUM_PIPAS
        ELSE 0 END PIPAS,  nvl(DF.FRECUENCIA, '--') FRECPIPAS, -- FIN INFO TIPO ABASTACEIMIENTO
--INICIA ALMACENAMIENTO
 CASE
         WHEN  DF.CISTERNA_ELEVADA = '1' THEN 'CISTERNA ELEVADA'
         WHEN  DF.CISTERNA_SUBTERRANEA = '1' THEN 'CISTERNA SUBTERRANEAL'
     ELSE '--' END  CISTERNA, nvl(DF.NUM_CISTERNAS, 0) CANTCISTERNA,
     CASE WHEN  DF.TONELES = 1 THEN
        DF.NUM_TONELES
        ELSE 0 END TONELES,--FIN  ALMACENAMIENTO
    --INICIA SERVICIOS
    DECODE( (NVL(DF.SANITARIOS, 0) ), 0, '--', DF.SANITARIOS ) SANITARIOS, DECODE( (NVL(DF.LAVAMANOS, 0) ), 0, '--', DF.LAVAMANOS ) LAVAMANOS,
    DECODE( (NVL(DF.LAVAPLATOS, 0) ), 0, '--', DF.LAVAPLATOS ) LAVAPLATOS, DECODE( (NVL(DF.DUCHAS, 0) ), 0, '--', DF.DUCHAS ) DUCHAS,
    DECODE( (NVL(DF.CHORROS, 0) ), 0, '--', DF.CHORROS ) CHORROS, DECODE( (NVL(DF.LAVADORA, 0) ), 0, '--', DF.LAVADORA ) LAVADORA,
    DECODE( (NVL(DF.PILA, 0) ), 0, '--', DF.PILA ) PILA, DECODE( (NVL(DF.PISCINA, 0) ), 0, '--', DF.PISCINA ) PISCINA,
    DECODE( (NVL(DF.M2JARDIN, 0) ), 0, '--', (DF.M2JARDIN ||' MTS2' )) M2JARDIN,--FIN SERVICIOS
     decode(TIPO_FICHA, 'F', 'FICHAJE', 'A', 'ALCANTARILLADO', 'R', 'RUTEO', 'FIC. Y ALCANT.') tipoFicha,
    ( decode(F.tipo_ficha, 'L', 'FA', F.TIPO_FICHA) ||'-'||F.SIGV2FICHASID) IDentificadorFicha,
 NVL( DF.CANDELAS_VISIBLES, 0)CANDELAS_VISIBLES, decode( DF.PLANTA_TRATAMIENTO , 1, 'SI', 'NO')PLANTA_TRATAMIENTO, nvl(DF.OBSERVACIONES2,'--') OBSERVACIONES2
FROM
    VICMAN.SIGV2_FICHAS F,
    VICMAN.SIGV2_DETALLE_FICHAS DF,
    VICMAN.SIGV2_ESTADOS_MEDIDOR EM
WHERE F.SIGV2DETALLECAMPANIASID = $P{p_detCampa}
AND F.SIGV2ESTADOSMEDIDORID = EM.SIGV2ESTADOSMEDIDORID
AND F.SIGV2FICHASID = DF.SIGV2FICHASID
AND DF.SIGV2ESTMEDALCANID IS NULL
AND F.TIPO_FICHA = 'R']]>
	</queryString>
