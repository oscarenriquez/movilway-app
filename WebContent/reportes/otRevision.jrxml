	<parameter name="p_orden" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT ('No.ORLC-' || rev_orden) revorden, rev_medidor, e.coddis zona,
       b.tcncli abonado, vicman.fnt_direcc (d.tcccli, d.cccodi) direccion,
       TO_CHAR (rev_fechagen, 'DD/MM/YYYY') rev_fechagen,
       NVL(rev_imp_contacto,' ') contacto, NVL(rev_imp_telefono,' ') telefono,
       rev_imp_tenencia tenenciainmueble, rev_imp_estado_inm statusinmueble,
       rev_ejecutado ejecutado, rev_medidor2 medidord, rev_imp_marca marca,
       rev_lectura_otv lectura, rev_medicion medicion,
       rev_resultado statusmed, rev_imp_tipo_inm tipinmueble,
       rev_resultado observaciones,
       TO_CHAR (rev_fecha, 'DD/MM/YYYY') fechaejecucion
  FROM dw_sig_ci_ordenes a,
       stcdatemp.v4client b,
       stcdatemp.v4serhed c,
       stcdatemp.v4conhed d,
       stcdatemp.v4clidir e
 WHERE a.rev_anexo = c.tcnfol
   AND c.tcccli = b.tcccli
   AND c.tcccli = d.tcccli
   AND d.tcccli = e.tcccli
   AND d.cccodi = e.cordir
   AND a.rev_orden  =$P{p_orden}]]>
	</queryString>
