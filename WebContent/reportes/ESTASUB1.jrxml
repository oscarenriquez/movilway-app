	<queryString>
		<![CDATA[SELECT
  T1.TCNCON CCOSTO, T5.TCCCLI, T4.TCNFOL ANEXO,
  T4.TCSERA MEDIDOR, T6.TCDESL DESC_ESTATUS,
  T7.TCNCLI NOMBRE,
  NVL(( SELECT MAX(CCDATD)
          FROM STCDATEMP.C4DOCTOS
         WHERE TCNCON=T1.TCNCON
           AND CCCABO='C'
           AND CCANUL!='A'
           AND CCSALT>0
           AND CCDATD>0 ),0) DIAS_MORA,
  STCDATEMP.FNT_DIRECC(T5.TCCCLI, T5.CCCODI) DIRECCION_INMUEBLE
FROM STCDATEMP.C4DOCTOS T1,
STCDATEMP.V4SERHED T4,
  STCDATEMP.V4CONHED T5,
  STCDATEMP.V4TABTES T6,
  STCDATEMP.V4CLIENT T7
WHERE T4.TCNCON(+)=T1.TCNCON AND
  T5.TCNCON=T1.TCNCON AND
  T6.TCSTTE(+)=T4.TCSTAA AND
  T7.TCCCLI(+)=T5.TCCCLI AND
  T4.TCNCON = $P{p_tcncon}
GROUP BY T1.TCNCON,T5.TCCCLI, T4.TCNFOL,
  T4.TCSERA, T6.TCDESL, T7.TCNCLI, T5.TCCCLI, T5.CCCODI]]>
