	<parameter name="p_tcncon" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT CASE WHEN CCCABO = 'C' THEN
            SUM(CCVDOC)
            ELSE 0
       END SUMA_CARGOS,
       CASE WHEN CCCABO = 'A' THEN
            SUM(CCVDOC)
            ELSE 0
       END SUMA_ABONOS,
       SUM(CCSALT)SALDO
FROM STCDATEMP.C4DOCTOS
WHERE CCANUL != 'A'
      AND TCNCON = $P{p_tcncon}
GROUP BY CCCABO]]>
	</queryString>
	<field name="SUMA_CARGOS" class="java.math.BigDecimal"/>
	<field name="SUMA_ABONOS" class="java.math.BigDecimal"/>
	<field name="SALDO" class="java.math.BigDecimal"/>
