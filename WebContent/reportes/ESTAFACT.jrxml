	<queryString>
		<![CDATA[SELECT
  T1.TCNCON,
  T1.CCTIPD||' '||T1.CCLDOC||' '||T1.CCNDOC DOC_CARGO,
  TO_DATE(T1.CCAZDO*10000+T1.CCMEDO*100+T1.CCDIDO,'YYYYMMDD') FECHA_EMI,
  TO_DATE(T1.CCAZVE*10000+T1.CCMEVE*100+T1.CCDIVE,'YYYYMMDD') FECHA_VENC,
  T1.CCVDOC MONTO_CARGO_Q,
  T1.CCSALT SALDO_CARGO_Q,
  NVL(T1.CCVDOC,0)-NVL(T1.CCSALT,0) TOT_ABONOS_Q,
  T2.TCSRIE||' '||T2.TCNIMP SERIE,
  T4.CCTPD2||' '||T4.CCLDC2||' '||T4.CCNDC2 DOC_ABONO,
  TO_DATE(T5.CCAZDO*10000+T5.CCMEDO*100+T5.CCDIDO,'YYYYMMDD') FECHA_ABONO,
  T5.CCUSRD USUARIO_ABONO,
  TO_DATE(T4.CCAZAF*10000+T4.CCMEAF*100+T4.CCDIAF,'YYYYMMDD') FECHA_AFEC,
  T4.CCVAAF
FROM
  STCDATEMP.C4DOCTOS T1,
  STCDATEMP.V4BILHED T2,
  STCDATEMP.C4AFECRU T4,
  STCDATEMP.C4DOCTOS T5
WHERE
  T2.TCTIRE=T1.CCTIPD AND
  T2.TCLERE=T1.CCLDOC AND
  T2.TCNURE=T1.CCNDOC AND
  T4.CCTPD1(+)=T2.TCTIRE AND
  T4.CCLDC1(+)=T2.TCLERE AND
  T4.CCNDC1(+)=T2.TCNURE AND
  T5.CCTIPD(+)=T4.CCTPD2 AND
  T5.CCLDOC(+)=T4.CCLDC2 AND
  T5.CCNDOC(+)=T4.CCNDC2 AND
  T1.CCCABO='C' AND
  T1.CCANUL!='A' AND
  T1.TCNCON = $P{p_tcncon}
UNION ALL
SELECT
  V1.TCNCON,
  V1.CCTIPD||' '||V1.CCLDOC||' '||V1.CCNDOC DOC_CARGO,
  V1 .FECHA_EMI, V1.FECHA_VENC,
  V1.MONTO_CARGO_Q,
  V1.SALDO_CARGO_Q,
  V1.TOT_ABONOS_Q,
  V1.SERIE||' '||V1.FACTURA SERIE,
  T2.CCTPD2||' '||T2.CCLDC2||' '||T2.CCNDC2 DOC_ABONO,
  TO_DATE(T3.CCAZDO*10000+T3.CCMEDO*100+T3.CCDIDO,'YYYYMMDD')FECHA_ABONO,
  T3.CCUSRD USUARIO_ABONO,
  TO_DATE(T2.CCAZAF*10000+T2.CCMEAF*100+T2.CCDIAF,'YYYYMMDD') FECHA_AFEC,
  T2.CCVAAF
FROM
    (
        SELECT T1.TCNCON,
               T1.CCTIPD, T1.CCLDOC, T1.CCNDOC,
            TO_DATE(T1.CCAZDO*10000+T1.CCMEDO*100+T1.CCDIDO,'YYYYMMDD') FECHA_EMI,
            TO_DATE(T1.CCAZVE*10000+T1.CCMEVE*100+T1.CCDIVE,'YYYYMMDD') FECHA_VENC,
            T1.CCVDOC MONTO_CARGO_Q,
            T1.CCSALT SALDO_CARGO_Q,
            NVL(T1.CCVDOC,0)-NVL(T1.CCSALT,0) TOT_ABONOS_Q,
            T2.FCSRIE SERIE, T2.FCDOIM FACTURA,
            T2.FCTOTF
        FROM STCDATEMP.C4DOCTOS T1,
             STCDATEMP.F4PRFHED T2
        WHERE T2.FCTDOC=T1.CCTIPD AND
              T2.FCLDOC=T1.CCLDOC AND
              T2.FCNDOC=T1.CCNDOC AND
              T1.CCCABO='C' AND
              T1.CCANUL!='A'
        ) V1,
  STCDATEMP.C4AFECRU T2,
  STCDATEMP.C4DOCTOS T3
WHERE
  T2.CCTPD1(+)=V1.CCTIPD AND
  T2.CCLDC1(+)=V1.CCLDOC AND
  T2.CCNDC1(+)=V1.CCNDOC AND
  T3.CCTIPD(+)=T2.CCTPD2 AND
  T3.CCLDOC(+)=T2.CCLDC2 AND
  T3.CCNDOC(+)=T2.CCNDC2 AND
  V1.TCNCON = $P{p_tcncon}
UNION ALL
  SELECT
  T1.TCNCON,
  T1.CCTIPD||' '||T1.CCLDOC||' '||T1.CCNDOC DOC_CARGO,
  TO_DATE(T1.CCAZDO*10000+T1.CCMEDO*100+T1.CCDIDO,'YYYYMMDD') FECHA_EMI,
  TO_DATE(T1.CCAZVE*10000+T1.CCMEVE*100+T1.CCDIVE,'YYYYMMDD') FECHA_VENC,
  T1.CCVDOC MONTO_CARGO_Q,
  T1.CCSALT SALDO_CARGO_Q,
  NVL(T1.CCVDOC,0)-NVL(T1.CCSALT,0) TOT_ABONOS_Q,
  NULL||' '||NULL,
  T2.CCTPD2||' '||T2.CCLDC2||' '||T2.CCNDC2 DOC_ABONO,
  TO_DATE(T3.CCAZDO*10000+T3.CCMEDO*100+T3.CCDIDO,'YYYYMMDD') FECHA_ABONO,
  T3.CCUSRD USUARIO_ABONO,
  TO_DATE(T2.CCAZAF*10000+T2.CCMEAF*100+T2.CCDIAF,'YYYYMMDD') FECHA_AFEC,
  T2.CCVAAF
FROM
  STCDATEMP.C4DOCTOS T1,
  STCDATEMP.C4AFECRU T2,
  STCDATEMP.C4DOCTOS T3
WHERE
  T2.CCTPD1(+)=T1.CCTIPD AND
  T2.CCLDC1(+)=T1.CCLDOC AND
  T2.CCNDC1(+)=T1.CCNDOC AND
  T3.CCTIPD(+)=T2.CCTPD2 AND
  T3.CCLDOC(+)=T2.CCLDC2 AND
  T3.CCNDOC(+)=T2.CCNDC2 AND
  T1.CCCABO='C' AND
  T1.CCANUL!='A' AND
  NOT EXISTS ( SELECT 1
                 FROM STCDATEMP.V4BILHED
                WHERE TCNCON=T1.TCNCON
                  AND TCTIRE=T1.CCTIPD
                  AND TCLERE=T1.CCLDOC
                  AND TCNURE=T1.CCNDOC ) AND
  NOT EXISTS ( SELECT 1
                 FROM STCDATEMP.F4PRFHED
                WHERE TCNCON=T1.TCNCON
                AND FCTDOC=T1.CCTIPD
                AND FCLDOC=T1.CCLDOC
                AND FCNDOC=T1.CCNDOC ) AND
  T1.TCNCON =$P{p_tcncon}
ORDER BY FECHA_EMI]]>
	</queryString>
